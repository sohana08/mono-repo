{
  "name": "@test-app/backend",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "--node-env=production"
          ],
          "cwd": "apps/backend"
        },
        "configurations": {
          "development": {
            "args": [
              "--node-env=development"
            ]
          }
        }
      },
      "prune-lockfile": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "executor": "@nx/js:prune-lockfile",
        "outputs": [
          "{workspaceRoot}/apps/backend/dist/package.json",
          "{workspaceRoot}/apps/backend/dist/package-lock.json"
        ],
        "options": {
          "buildTarget": "build"
        }
      },
      "copy-workspace-modules": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "outputs": [
          "{workspaceRoot}/apps/backend/dist/workspace_modules"
        ],
        "executor": "@nx/js:copy-workspace-modules",
        "options": {
          "buildTarget": "build"
        }
      },
      "prune": {
        "dependsOn": [
          "prune-lockfile",
          "copy-workspace-modules"
        ],
        "executor": "nx:noop"
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@test-app/backend:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@test-app/backend:build:development"
          },
          "production": {
            "buildTarget": "@test-app/backend:build:production"
          }
        }
      },
      "docker:build": {
        "dependsOn": [
          "build",
          "prune"
        ]
      },
      "db:create": {
        "executor": "nx:run-commands",
        "options": {
          "command": "node -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli migration:create apps/backend/src/database/migrations/Empty",
          "cwd": "{workspaceRoot}"
        }
      },
      "db:generate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "node -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli migration:generate -d apps/backend/ormconfig.ts apps/backend/src/database/migrations/Auto",
          "cwd": "{workspaceRoot}"
        }
      },
      "db:run": {
        "executor": "nx:run-commands",
        "options": {
          "command": "node -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli migration:run -d apps/backend/ormconfig.ts",
          "cwd": "{workspaceRoot}"
        }
      },
      "db:revert": {
        "executor": "nx:run-commands",
        "options": {
          "command": "node -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli migration:revert -d apps/backend/ormconfig.ts",
          "cwd": "{workspaceRoot}"
        }
      }
    }
  },
  "dependencies": {
    "@types/compression": "^1.8.1",
    "@types/morgan": "^1.9.10",
    "class-transformer": "^0.5.1",
    "mysql2": "^3.15.1"
  }
}
